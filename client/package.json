{
  "name": "react-mobx-mui-ts-boilerplate",
  "version": "1.0.0",
  "description": "A minimal React + MobX + Material UI boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:file": "jest Theme.test.tsx",
    "setup": "npm install && npm run build",
    "dev": "webpack serve --config config/dev.js",
    "build": "webpack --config config/prod.js"
  },
  "keywords": [
